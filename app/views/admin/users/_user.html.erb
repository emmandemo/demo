<tr>
<td><%= link_to user.name, admin_user_path(user) %></td>
<td><%= user.email %></td>
<td><%= user.featured? %></td>
<td><%= user.created_at %></td>
<td class="actions"><%= link_to "Edit", edit_admin_user_path(user), class: "btn btn-primary btn-xs" %>
 <% unless user == current_user %>
  <%= link_to "Delete", admin_user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this person?" }, class: "btn btn-danger btn-xs" %><% end %></td>
</tr>
